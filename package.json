    "@ctrl/tinycolor": "^3.0.2",
    "axios": "^1.6.2",
    "blingblingjs": "^2.1.1",
    "construct-style-sheets-polyfill": "^2.4.2",
    "cors": "^2.8.5",
    "escodegen": "^2.1.0",
    "hotkeys-js": "^3.7.6",
    "html-react-parser": "^5.0.6",
    "http-proxy-middleware": "^2.0.6",
    "ignore": "^5.3.0",
